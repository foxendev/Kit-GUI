//Draw all instances
var i, elm_count, o;
elm_count = ds_list_size(global._kit_instance_order)
for (i=0; i&lt;elm_count; i+=1)
{
    o = global._kit_instance_order[| i]
    if (o.is_visible)
    {
        if o.master_id &gt; -1
        {
            if ((o.master_id).is_visible = false) or ((o.master_id).__v = false)
            {
                o.__v = false
                continue;
            }
        }
        kit_element_update_pos(o)
        with (o)
        {
            __v = true
            event_perform(ev_draw, 0)
        }
    } else
    {
        o.__v = false
    }
};
